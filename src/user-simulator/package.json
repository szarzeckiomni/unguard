{
    "name": "user-simulator",
    "version": "1.0.0",
    "description": "Simulates a real browser unguard user.",
    "scripts": {
        "start": "element run default-user-sim.perf.ts"
    },
    "private": true,
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 100,
        "useTabs": true,
        "tabWidth": 2,
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "arrowParens": "avoid"
    },
    "dependencies": {
        "@flood/element": "^2.0.4",
        "@flood/element-cli": "^2.0.4",
        "prettier": "^2.8.8"
    },
    "devDependencies": {
        "@types/node": "18.11.18"
    },
    "element": {
        "testMatch": [
            "**/*.perf.[tj]s"
        ]
    }
}
